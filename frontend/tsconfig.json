{
	"compilerOptions": {
		"target": "es5",
		"module": "esnext",
		"moduleResolution": "node",
		"noResolve": false,
		"noImplicitAny": false,
		"removeComments": false,
		"jsx": "react",
		"allowSyntheticDefaultImports": true,
		"keyofStringsOnly": true
	},
	"typeRoots": ["node_modules/@types", "src/@type"],
	"exclude": ["node_modules", "build", "scripts", "webpack", "./node_modules/**/*"],
	"include": ["./src/**/*", "@type", "custom.d.ts"]
}
